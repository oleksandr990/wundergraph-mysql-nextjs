{
  "name": "nextjs-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run nextDev\" \"npm run database\" \"npm run wundergraph\" \"npm run browser\"",
    "wundergraph": "wait-on \"tcp:54322\" && wunderctl up --debug",
    "browser": "wait-on \"http-get://localhost:3000\" && wait-on \"http-get://localhost:9991\" && open-cli http://localhost:3000",
    "build": "next build",
    "database": "docker-compose up",
    "nextDev": "next dev",
    "nextStart": "next start",
    "generate": "cd .wundergraph && wunderctl generate --debug",
    "wunderctl": "wunderctl"
  },
  "dependencies": {
    "@emotion/cache": "^11.10.2",
    "@emotion/react": "^11.10.0",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/lab": "^5.0.0-alpha.96",
    "@mui/material": "^5.10.2",
    "@wundergraph/nextjs": "^0.0.35",
    "@wundergraph/sdk": "^0.96.1",
    "apexcharts-clevision": "^3.28.5",
    "clsx": "^1.2.1",
    "graphql": "^16.3.0",
    "mdi-material-ui": "^7.5.0",
    "next": "^12.1.6",
    "nprogress": "^0.2.0",
    "react": "^18.0.0",
    "react-apexcharts": "^1.4.0",
    "react-datepicker": "^4.8.0",
    "react-dom": "^18.2.0",
    "react-perfect-scrollbar": "^1.5.8",
    "react-popper": "^2.3.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.45",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "concurrently": "^6.0.0",
    "eslint-config-next": "^12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.0",
    "eslint-plugin-import": "^2.26.0",
    "next-transpile-modules": "^9.0.0",
    "open-cli": "^7.0.1",
    "prettier": "^2.7.1",
    "typescript": "^4.7.2",
    "wait-on": "^6.0.0"
  }
}
